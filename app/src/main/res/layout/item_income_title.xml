<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white">
    <RelativeLayout
        android:id="@+id/item_income_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="5dp">
        <TextView
            android:id="@+id/item_income_titless"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="新增代理"
            android:textStyle="bold"
            android:textSize="18sp"
            android:textColor="@color/home_word"
            android:layout_centerVertical="true"/>
        <TextView
            android:id="@+id/item_income_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="更多"
            android:textColor="@color/comment_word"
            android:textSize="14sp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:padding="10dp"/>
    </RelativeLayout>

    <com.jzkl.util.MyListView
        android:id="@+id/item_income_list2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@color/transparent"
        android:dividerHeight="0dp"/>
</LinearLayout>
